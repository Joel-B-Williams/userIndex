<div>
  <div class="list_container">
    <div class="user_list">
      <ul>
        <% @users.each do |user| %>
          <li class="user_list_item">
            <%= user.first_name%>
            <% if policy(user).edit? %>
            <%= link_to 'Edit', edit_user_registration_path(user) %>
            <% end %>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>