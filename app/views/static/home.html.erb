<div>
  <div class="user_list">
    <ul>
      <% @users.each do |user| %>
        <li>
          <%= user.first_name%>
          <% if policy(user).edit? %>
          <%= link_to 'Edit', edit_user_registration_path(user) %>
          <% end %>
        </li>
      <% end %>
    </ul>
  </div>
</div>